! Образец для ДЗ2

FINISH
/CLEAR

! Начальные параметры
L = 0.15                ! Длина балки
B = 0.008               ! Ширина балки
H = 0.002               ! Высота балки
E = 2e11                ! Модуль упругости материала
MU = 0.3                ! Коэффициент Пуассона материала
M = 0.1                 ! Масса

! Выбор КЭ
/ PREP7
ET,1,BEAM188,,,3       ! Выбираем тип элемента для балки с 3 узлами
ET,2,MASS21,,,4        ! Выбираем тип элемента для сосредоточенной массы (без инерции)
ET,3,MASS21,,,3        ! Выбираем тип элемента для сосредоточенной массы (с инерцией)

! Задаем свойства материала КЭ1
MP,EX,1,E               ! Задаем модуль упругости материала
MP,PRXY,1,MU            ! Задаем коэффициент Пуассона материала

! Задание сечения КЭ1
SECTYPE,1,BEAM,RECT     ! Задаем сечение балки прямоугольной формы
SECDATA,B,H             ! Задаем ширину и высоту сечения

! Задаем свойства материала КЭ2 и КЭ3
R,2,M*2                 ! Задаем массу без инерции для КЭ2
R,3,M,M*L*L/12          ! Задаем массу с инерцией для КЭ3

! Моделирование

! Узлы
N,1,0,0,0               ! Создаем узел 1 в начале координат
N,2,L,0,0               ! Создаем узел 2 с координатами (L, 0, 0)
N,3,2*L,0,0             ! Создаем узел 3 с координатами (2L, 0, 0)
N,4,0,L/4,0             ! Создаем узел 4 с координатами (0, L/4, 0)

! Построение стержней
TYPE,1
MAT,1
SECNUM,1
E,1,2,4                ! Создаем балку между узлами 1, 2 и 4
E,2,3,4                ! Создаем балку между узлами 2, 3 и 4

! Построение сосредаточенной массы в узле 2
TYPE,3
REAL,3
E,2

! Построение массы без инерции в узле 3
TYPE,2
REAL,2
E,3

/VIEW,1,1,1,1           ! Устанавливаем вид для графического вывода (изометрия)
/eshape,1                ! Устанавливаем форму элементов для реалистичного изображения масс
GPLOT                    ! Визуализируем результат

! Закрепления
/SOL
D,1,ALL,0                ! Задаем условия нулевого перемещения для узла 1
/PBC,ALL,,1              ! Задаем точки фиксации вдоль координаты Z
GPLOT

! Первый скриншот

!DSYM,SYMM,Z		 ! Нужно в дз!!! для превращения задачи в плоскую

D,ALL,UX,0               ! Растяженимем и сжатием  пренебрегаем
ANTYPE,2                 ! Расчет устойчивости
MODOPT,LANB,4,1e-3,1000  ! Опции для расчета устойчивости
MXPAND,4,,,1             ! Просмотр нормального деформированного состояния с eshape

! Расчет
SOLVE
/POST1
SET,LIST

! Второй скриншот (возможно частоты будут в консоле)

SET,,1
PLDISP,1

! Последний скриншот

